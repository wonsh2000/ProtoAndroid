<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!--For Medium Font Size-->
    <style name="Medium12" parent="KakaotestFont">
        <item name="android:textSize">12dp</item>
    </style>

    <style name="Bold12" parent="KakaotestFont">
        <item name="android:textSize">12dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Bold5" parent="KakaotestFont">
        <item name="android:textSize">5dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="KakaotestFont">
        <item name="android:includeFontPadding">false</item>
        <item name="android:lineSpacingExtra">@dimen/line_spacing_0</item>
    </style>

    <style name="CustomDialogTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>


</resources>